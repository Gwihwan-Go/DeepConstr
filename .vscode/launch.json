        {
            "name": "fuzz(neuri)",
            "type": "python",
            "request": "launch",
            "program": "neuri/cli/fuzz.py",
            "console": "integratedTerminal",
            "env" : {
                "PYTHONPATH": "/artifact:/artifact/neuri"
            },
            "args": ["fuzz.time=120m",
                    "mgen.record_path=data/torch_records",
                    "fuzz.root=gen/debug_neuri",
                    "filter.type=['nan','dup','inf']",
                    "backend.type=torchjit",
                    "mgen.test_pool=['Slice', 'torch.Tensor.relu_', 'torch.Tensor.diagonal', 'torch.Tensor.gt']",
                    "model.type=torch",
                    "fuzz.save_test=gen/debug_neuri.models",
                    "debug.viz=true",
                    "hydra.verbose=fuzz",
                    "fuzz.resume=false",
                    "mgen.method=neuri",
                    "mgen.max_nodes=3",
        ],
            "justMyCode": true
        },
        {
            "name": "fuzz(constrinf)",
            "type": "python",
            "request": "launch",
            "program": "neuri/cli/fuzz.py",
            "console": "integratedTerminal",
            "env" : {
                "PYTHONPATH": "/artifact/:/artifact/neuri/"
            },
            "args": ["fuzz.time=120m",
                    "mgen.record_path=data/constraints/torch",
                    "fuzz.root=gen/torch-constrinf-n3",
                    "filter.type=['nan','dup','inf']",
                    "backend.type=torchcomp",
                    "model.type=torch",
                    "fuzz.save_test=gen/torch-constrinf-n3.models",
                    "debug.viz=true",
                    "hydra.verbose=fuzz",
                    "fuzz.resume=false",
                    "mgen.method=constrinf",
                    "mgen.max_nodes=3",
                    "mgen.test_pool=['torch.as_tensor', 'Slice']",
                    // "mgen.test_pool=['torch.acos', 'Slice']",
                    // "mgen.test_pool=['torch.addr', 'torch.as_strided', 'torch.atan2', 'torch.cross', 'torch.floor', 'torch.imag', 'torch.logaddexp2']",
        ],
            "justMyCode": true
